{
  "name": "n8n-nodes-telegram-client",
  "version": "0.0.21",
  "description": "n8n node for Telegram Client API with full user capabilities",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ofekb/n8n-nodes-telegram-client",
  "author": {
    "name": "Ofek",
    "telegram": "@AppDev420"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ofekb/n8n-nodes-telegram-client.git"
  },
  "main": "nodes/TelegramClient/TelegramClient.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TelegramClient.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TelegramClient/TelegramClient.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "n8n-workflow": "~1.5.0",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "telegram": "^2.19.10",
    "gulp": "^5.0.0",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.48.0"
  }
}
