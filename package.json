{
  "name": "n8n-nodes-telegram-client",
  "version": "0.0.18",
  "description": "n8n node for Telegram Client API with full user capabilities",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ofekb/n8n-nodes-telegram-client",
  "author": {
    "name": "Ofek",
    "telegram": "@AppDev420"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ofekb/n8n-nodes-telegram-client.git"
  },
  "main": "dist/nodes/TelegramClientNode.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TelegramClient.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TelegramClientNode.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "telegram": "^2.19.10"
  },
  "peerDependencies": {
    "n8n-core": ">=1.0.0",
    "n8n-workflow": ">=1.0.0"
  }
}
