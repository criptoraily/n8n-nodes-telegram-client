{
  "name": "n8n-nodes-telegram-client",
  "version": "0.0.26",
  "description": "n8n node for Telegram Client API with full user capabilities",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ofekb/n8n-nodes-telegram-client",
  "author": {
    "name": "Ofek",
    "telegram": "@AppDev420"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ofekb/n8n-nodes-telegram-client.git"
  },
  "main": "nodes/TelegramClient/TelegramClient.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TelegramClient.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TelegramClient/TelegramClient.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/form-data": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "form-data": "^4.0.1",
    "gulp": "^4.0.2",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.48.0",
    "prettier": "^2.8.0",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "form-data": "^2.5.2",
    "axios": "^1.7.7",
    "path": "^0.12.7"
  }
}
