{
    "compilerOptions": {
      "lib": ["es2019"],
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": ".",
      "target": "es2019",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "declaration": true
    },
    "include": ["nodes/**/*", "credentials/**/*"],
    "exclude": ["node_modules/**/*", "dist/**/*"]
  }